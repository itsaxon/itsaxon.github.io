#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/workspaces/itsaxon.github.io/vuepress-starter/node_modules/.pnpm/vuepress@2.0.0-rc.14_@vuepress+bundler-vite@2.0.0-rc.7_@types+node@22.1.0_sass@1.77.8__vue@3.4.35/node_modules/vuepress/bin/node_modules:/workspaces/itsaxon.github.io/vuepress-starter/node_modules/.pnpm/vuepress@2.0.0-rc.14_@vuepress+bundler-vite@2.0.0-rc.7_@types+node@22.1.0_sass@1.77.8__vue@3.4.35/node_modules/vuepress/node_modules:/workspaces/itsaxon.github.io/vuepress-starter/node_modules/.pnpm/vuepress@2.0.0-rc.14_@vuepress+bundler-vite@2.0.0-rc.7_@types+node@22.1.0_sass@1.77.8__vue@3.4.35/node_modules:/workspaces/itsaxon.github.io/vuepress-starter/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/workspaces/itsaxon.github.io/vuepress-starter/node_modules/.pnpm/vuepress@2.0.0-rc.14_@vuepress+bundler-vite@2.0.0-rc.7_@types+node@22.1.0_sass@1.77.8__vue@3.4.35/node_modules/vuepress/bin/node_modules:/workspaces/itsaxon.github.io/vuepress-starter/node_modules/.pnpm/vuepress@2.0.0-rc.14_@vuepress+bundler-vite@2.0.0-rc.7_@types+node@22.1.0_sass@1.77.8__vue@3.4.35/node_modules/vuepress/node_modules:/workspaces/itsaxon.github.io/vuepress-starter/node_modules/.pnpm/vuepress@2.0.0-rc.14_@vuepress+bundler-vite@2.0.0-rc.7_@types+node@22.1.0_sass@1.77.8__vue@3.4.35/node_modules:/workspaces/itsaxon.github.io/vuepress-starter/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/vuepress.js" "$@"
else
  exec node  "$basedir/../../bin/vuepress.js" "$@"
fi
